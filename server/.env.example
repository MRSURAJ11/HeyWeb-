# .env.example for HeyWeb!
# -------------------------------------------
# Copy this file to `.env` (keep it out of Git) and replace the placeholder
# values with your own configuration secrets / URLs.

# OpenAI / OpenRouter settings
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-5
API_BASE_URL=https://openrouter.ai/api/v1

# Front-end origin allowed for CORS
FRONTEND_ORIGIN=http://localhost:5173

# Port the Express server will listen on
PORT=4000
